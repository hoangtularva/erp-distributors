<p id="notice"><%= notice %></p>

<h1>Distributors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @distributors.each do |distributor| %>
      <tr>

        <td><%= link_to 'Edit', edit_backend_distributor_path(distributor) %></td>
        <td><%= button_to 'Destroy', backend_distributor_path(distributor), method: :delete%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Distributor', new_backend_distributor_path %>
